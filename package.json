{
  "scripts": {
    "build:dev": "run-p build:dev:*",
    "build:dev:css": "parcel watch src/frontend/scss/pages/**/main.scss -d ./src/assets/css",
    "build:devd:ts": "parcel watch ./src/frontend/ts/pages/**/main.ts -d ./src/assets/js",
    "build:prod": "run-p build:prod:*",
    "build:prod:next": "next build",
    "check-all": "run-s check-code test",
    "check-code": "run-p lint check-spell check-type",
    "check-spell": "cspell './src/**/*.ts' './spec/api/**'",
    "check-type": "tsc",
    "db": "docker-compose up",
    "dev": "npm-run-all -p db build:dev serve:dev",
    "fix": "run-p fix:*",
    "fix:config": "prettier ./.eslintrc.js ./*.js --write",
    "fix:json": "prettier ./*.json ./**/*.json --write",
    "fix:pack": "fixpack",
    "fix:scss": "stylelint './src/frontend/scss/**/*.scss' --fix",
    "fix:ts": "eslint './src/**/*.ts' --fix",
    "lint": "run-p lint:*",
    "lint:pug": "pug-lint src",
    "lint:scss": "stylelint './src/frontend/scss/**/*.scss'",
    "lint:ts": "eslint './src/**/*.ts'",
    "serve:dev": "nodemon",
    "serve:prod": "ts-node  --files ./src/index.ts",
    "start": "NODE_ENV=production npm-run-all build:prod -p serve:prod db",
    "test": "jest"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@specter/client": "^0.6.5",
    "@specter/redux-effects-specter": "^0.6.5",
    "@specter/specter": "^0.6.5",
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.6.0",
    "fastify-compress": "^3.3.1",
    "fastify-formbody": "^5.0.0",
    "fastify-helmet": "^5.0.3",
    "fastify-nextjs": "^5.1.0",
    "fastify-static": "^3.2.1",
    "middie": "^5.2.0",
    "mongoose": "^5.10.9",
    "next": "^9.5.5",
    "point-of-view": "^4.6.0",
    "pug": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1",
    "redux-effects-formdata-uploader": "^1.4.1",
    "redux-effects-steps": "^1.0.3",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.10",
    "@types/pug": "^2.0.4",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "cspell": "^4.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "fixpack": "^3.0.6",
    "jest": "^26.5.3",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "pug-lint": "^2.6.0",
    "pug-lint-config-clock": "^2.0.0",
    "sass": "^1.27.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
