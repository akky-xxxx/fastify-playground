{
  "scripts": {
    "check-all": "run-s check-code test",
    "check-code": "run-p lint check-spell check-type",
    "check-spell": "cspell './src/**/*.ts' './spec/api/**'",
    "check-type": "tsc",
    "dev": "nodemon",
    "fix": "run-p fix:*",
    "fix:config": "prettier ./.eslintrc.js ./*.js --write",
    "fix:json": "prettier ./*.json ./**/*.json --write",
    "fix:pack": "fixpack",
    "fix:ts": "eslint './src/**/*.ts' --fix",
    "lint": "run-p lint:*",
    "lint:pug": "pug-lint src",
    "lint:ts": "eslint './src/**/*.ts'",
    "start": "NODE_ENV=production ts-node  --files ./src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^3.6.0",
    "fastify-compress": "^3.3.1",
    "fastify-helmet": "^5.0.3",
    "fastify-static": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "cspell": "^4.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "fixpack": "^3.0.6",
    "jest": "^26.5.3",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "name": "11.fastify-playground",
