{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "downlevelIteration": true,
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "moduleResolution": "Node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "../../assets/js"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.js.ts"]
}
